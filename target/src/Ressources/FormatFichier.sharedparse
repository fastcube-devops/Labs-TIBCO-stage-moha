<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <name>FormatFichier</name>
    <resourceType>ae.shared.ParseSharedResource</resourceType>
    <config xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <FormatType>Delimiter separated</FormatType>
        <ColumnSeparator>,</ColumnSeparator>
        <ColSeparatorRule>Treat all characters as entered as a single column separator string</ColSeparatorRule>
        <LineSeparator>
</LineSeparator>
        <FillCharacter>Space</FillCharacter>
        <LineLength>1</LineLength>
        <DataFormat>
            <xsd:element name="Client">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="id_client" type="xsd:string"/>
                        <xsd:element name="nom" type="xsd:string"/>
                        <xsd:element name="prenom" type="xsd:string"/>
                        <xsd:element name="adresse" type="xsd:string"/>
                        <xsd:element name="profession" type="xsd:string"/>
                        <xsd:element name="date_ajout" type="xsd:string"/>
                        <xsd:element name="date_update" type="xsd:string"/>
                        <xsd:element name="flag" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </DataFormat>
    </config>
</BWSharedResource>